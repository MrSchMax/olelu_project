// PT Sans Font
@font-face
  font-family: 'PT Sans'
  font-style: normal
  font-weight: normal

	src: url('/assets/fonts/ptsans/pts55f_w.eot')
	src: local('PT Sans'),
       url('/assets/fonts/ptsans/PTS55F_W.eot?#iefix') format('embedded-opentype'),
       url('/assets/fonts/ptsans/PTS55F_W.svg#PTSans-Regular') format('svg'),
       url('/assets/fonts/ptsans/PTS55F_W.woff') format('woff'),
       url('/assets/fonts/ptsans/PTS55F_W.ttf') format('truetype')

@font-face
  font-family: 'PT Sans'
  font-style: italic
  font-weight: normal

	src: url('/assets/fonts/ptsans/pts56f_w.eot')
